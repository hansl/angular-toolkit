{
  "compilerOptions": {
    "baseUrl": ".",
    "declaration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "outDir": "../dist/",
    "sourceMap": true,
    "target": "es5",
    "paths": {
      "@angular/services/*": [
        "../src/*"
      ]
    }
  }
}
